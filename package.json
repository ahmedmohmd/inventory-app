{
	"name": "tech-vault",
	"version": "1.0.0",
	"main": "app.js",
	"scripts": {
		"start:dev": "nodemon app.ts",
		"start:prod": "node ./dist/app.js",
		"build": "tsc && copyfiles swagger.json dist",
		"copy": "cpx 'swagger.json' dist",
		"format": "prettier . --write",
		"lint": "eslint .",
		"test:run": "vitest",
		"test:coverage": "vitest run --coverage",
		"test:preview": "npx vite preview --outDir html",
		"prepare": "husky",
		"update:check": "npm-check --skip-unused ",
		"update:run": "npm-check --update --skip-unused",
		"check-unused-pkgs": "depcheck",
		"add-types": "typesync"
	},
	"lint-staged": {
		"*.{js,ts}": [
			"eslint --quiet --fix"
		],
		"*.{json,js,ts}": [
			"prettier --write --ignore-unknown"
		]
	},
	"keywords": [
		"inventory-app",
		"tech vault",
		"tech-vault"
	],
	"author": "Ahmed Muhammad",
	"license": "ISC",
	"private": true,
	"description": "Simple Description for the App",
	"devDependencies": {
		"@eslint/js": "^9.8.0",
		"@types/bcrypt": "~10.0.28-alpha",
		"@types/compression": "~1.7.5",
		"@types/copyfiles": "^2.4.4",
		"@types/cors": "~2.8.17",
		"@types/eslint": "~9.6.0",
		"@types/eslint__js": "~8.42.3",
		"@types/eslint-config-prettier": "~6.11.3",
		"@types/express": "~4.17.21",
		"@types/http-errors": "~2.0.4",
		"@types/http-status-codes": "^1.2.0",
		"@types/jsonwebtoken": "~9.0.6",
		"@types/lint-staged": "~13.3.0",
		"@types/node": "^22.1.0",
		"@types/nodemailer": "^6.4.15",
		"@types/pg": "^8.11.6",
		"@types/supertest": "~6.0.2",
		"@types/swagger-ui-express": "^4.1.6",
		"@vitest/coverage-v8": "^2.0.5",
		"@vitest/ui": "^2.0.5",
		"drizzle-kit": "^0.24.0",
		"eslint": "^9.8.0",
		"eslint-config-prettier": "^9.1.0",
		"globals": "^15.9.0",
		"husky": "^9.1.4",
		"lint-staged": "^15.2.8",
		"nodemon": "^3.1.4",
		"prettier": "^3.3.3",
		"supertest": "^7.0.0",
		"ts-node": "^10.9.2",
		"typescript": "^5.5.4",
		"typescript-eslint": "^8.0.1",
		"typesync": "^0.13.0",
		"vitest": "^2.0.5"
	},
	"dependencies": {
		"@types/multer": "^1.4.11",
		"bcrypt": "^5.1.1",
		"cloudinary": "^2.4.0",
		"compression": "^1.7.4",
		"copyfiles": "^2.4.1",
		"cors": "^2.8.5",
		"cpx": "^1.5.0",
		"dotenv": "^16.4.5",
		"drizzle-orm": "^0.33.0",
		"express": "^4.19.2",
		"express-async-errors": "^3.1.1",
		"express-rate-limit": "^7.4.0",
		"helmet": "^7.1.0",
		"http-errors": "^2.0.0",
		"http-status-codes": "^2.3.0",
		"jsonwebtoken": "^9.0.2",
		"multer": "^1.4.5-lts.1",
		"nodemailer": "^6.9.14",
		"pg": "^8.12.0",
		"swagger-ui-express": "^5.0.1",
		"winston": "^3.13.1",
		"winston-daily-rotate-file": "^5.0.0",
		"zod": "^3.23.8"
	}
}
